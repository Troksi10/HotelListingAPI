{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsot.Hosting.LifeTime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.text",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {"ServerUrl": "http://localhost:5341"}
      }
    ]
  },
  "AllowedHosts": "*"
}
